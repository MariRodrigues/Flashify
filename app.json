{
  "expo": {
    "name": "flashify",
    "slug": "flashify",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/flashify.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/starting.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/flashify.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.srtamarisouza.flashify"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "d429a4f4-9e3a-4b4d-99a1-9084417b1a31"
      }
    },
    "owner": "srtamarisouza"
  }
}
